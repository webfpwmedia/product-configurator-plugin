{
    "name": "arcpointgroup/product-configurator",
    "description": "Arc Point Group Product Configurator Plugin",
    "homepage": "https://github.com/arcpointgroup/product-configurator",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=7.2",
        "ext-json": "^1.7",
        "cakephp/cakephp": "3.7.*",
        "cakephp/migrations": "^2.0.0",
        "league/flysystem": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5|^6",
        "psy/psysh": "@stable"
    },
    "autoload": {
        "psr-4": {
            "ARC\\ProductConfigurator\\": "src/"
        }
    },
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
